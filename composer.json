{
    "name": "elriri35/cakephp-duplidraft",
    "description": "CakePHP ORM plugin for duplicating entities (including related entities) Forked from riesenia/cakephp-duplicatable ",
    "keywords": ["cakephp", "orm", "copy", "duplicate"],
    "type": "cakephp-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Tomas Saghy",
            "email": "segy@riesenia.com"
        }
    ],
    "require": {
        "php": ">=5.4.16",
        "cakephp/orm": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "cakephp/cakephp": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Duplidraft\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Duplidraft\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
